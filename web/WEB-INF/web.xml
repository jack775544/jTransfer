<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>jTransfer.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>TransferServlet</servlet-name>
        <servlet-class>jTransfer.TransferServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TransferServlet</servlet-name>
        <url-pattern>/files</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListFilesServlet</servlet-name>
        <servlet-class>jTransfer.ListFilesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListFilesServlet</servlet-name>
        <url-pattern>/ls</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetFileServlet</servlet-name>
        <servlet-class>jTransfer.GetFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetFileServlet</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EditFileServlet</servlet-name>
        <servlet-class>jTransfer.EditFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EditFileServlet</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>jTransfer.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PwdServlet</servlet-name>
        <servlet-class>jTransfer.PwdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PwdServlet</servlet-name>
        <url-pattern>/pwd</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadFileServlet</servlet-name>
        <servlet-class>jTransfer.UploadFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadFileServlet</servlet-name>
        <url-pattern>/put</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ExecServlet</servlet-name>
        <servlet-class>jTransfer.ExecServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExecServlet</servlet-name>
        <url-pattern>/exec</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>jTransfer.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadRawServlet</servlet-name>
        <servlet-class>jTransfer.UploadRawServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadRawServlet</servlet-name>
        <url-pattern>/raw</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteFileServlet</servlet-name>
        <servlet-class>jTransfer.DeleteFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteFileServlet</servlet-name>
        <url-pattern>/rm</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RenameFileServlet</servlet-name>
        <servlet-class>jTransfer.RenameFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RenameFileServlet</servlet-name>
        <url-pattern>/rename</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>jTransfer.AdminServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>jTransfer.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/adminLogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminValidationServlet</servlet-name>
        <servlet-class>jTransfer.AdminValidationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminValidationServlet</servlet-name>
        <url-pattern>/adminValidate</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChangePasswordServlet</servlet-name>
        <servlet-class>jTransfer.ChangePasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePasswordServlet</servlet-name>
        <url-pattern>/changePass</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PasswordChangeInterfaceServlet</servlet-name>
        <servlet-class>jTransfer.PasswordChangeInterfaceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PasswordChangeInterfaceServlet</servlet-name>
        <url-pattern>/change</url-pattern>
    </servlet-mapping>

    <listener>
        <listener-class>jTransfer.TransferSessionListener</listener-class>
    </listener>
</web-app>